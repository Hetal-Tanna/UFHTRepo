																	image="{!IF(hasNext,URLFOR($Resource.BootStrap,'/bootstrapNew/images/latest-arrow-disabled.png'),
                                                                        URLFOR($Resource.BootStrap,'/bootstrapNew/images/latest-arrow.png'))}"
																	styleclass="last-btn" disabled="{!hasNext}"
																	reRender="renderHyperLink"
																	style="background-color:#fff !important;    border: none; vertical-align: bottom;"
																	status="actStatusId" />
															</apex:outputPanel>
														</div>
													</div>
												</div>
											</apex:outputPanel>
											<apex:outputPanel
												rendered="{!AND(selectedObject != '  ', displayHistoryWrapperList.size == 0)}">
												<apex:pageMessage summary="{!$Label.Information_Message}"
													severity="info" strength="2" />
											</apex:outputPanel>
											<apex:outputPanel
												rendered="{!AND(selectedObject == '  ',displayHistoryWrapperList.size == 0)}">
												<apex:pageMessage
													summary="{!$Label.Information_Message_on_View_Page}"
													severity="info" strength="2" />
											</apex:outputPanel>
										</apex:outputPanel>
									</div>
								</div>
							</apex:outputPanel>
						</div>
						<apex:actionFunction name="DateFileter" action="{!dateFilter}"
							reRender="CustomDate, renderHyperLink, downloadLinks,viewMoreData "
							status="actStatusId"
							oncomplete="validateDate(false), hideDateFormat(); disableDateFields();" />
						<apex:actionFunction name="DateFileterWhenStartEndDateNull"
							action="{!dateFileterWhenStartEndDateNull}"
							reRender="DateFilter, renderHyperLink, downloadLinks, viewMoreData"
							status="actStatusId" oncomplete="hideDateFormat();" />
					</apex:form>
				</div>
			</div>
		</div>
	</div>
</apex:page>